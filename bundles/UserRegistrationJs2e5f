function generatePaging(n,t,i){var r,u;if($(".ellipsis").remove(),current=t,totalPages=n,r="",n>1){for(t!==1?(r+="<li class='pagination-previous' style='display: none;'><a href='javascript:void(0);' onclick='previousnextclick(-2);return false;' class='circleicn'><em class='fa fa-backward'><\/em><span class='show-for-sr'>If pagination previous click<\/span><\/a><\/li>",r+="<li class='pagination-previous' style='display: none;'><a href='javascript:void(0);' onclick='previousnextclick(-1);return false;' class='circleicn caret'><em class='fa fa-caret-left'><\/em><span class='show-for-sr'>If pagination previous forward click<\/span><\/a><\/li>"):(r+="<li class='pagination-previous' style='display: none;'><a href='javascript:void(0);' onclick='previousnextclick(-2);return false;' class='circleicn'><em class='fa fa-backward'><\/em><span class='show-for-sr'>Else pagination previous click<\/span><\/a><\/li>",r+="<li class='pagination-previous' style='display: none;'><a href='javascript:void(0);' onclick='previousnextclick(-1);return false;' class='circleicn caret'><em class='fa fa-caret-left'><\/em><span class='show-for-sr'>Else pagination previous forward click<\/span><\/a><\/li>"),u=1;u<=n;u++)u===1?r+='<li class="current all pno'+u+'">':u>4&&u<n-1?r+='<li style="display:none;" class="all pno'+u+'">':(n>6&&u===n-1&&(r+='<li class="ellipsis" aria-hidden="true"><\/li>'),r+='<li class="all pno'+u+'">'),r+='<a href="#" onclick="pageclick('+u+",'','paging');return false;\" aria-label=\"Page "+u+'">'+u+"<\/a><\/li>";t!==n?(r+='<li class="pagination-next"><a href="javascript:void(0);" onclick="previousnextclick(1);return false;" class="circleicn caret"><em class="fa fa-caret-right"><\/em><span class="show-for-sr">If pagination next click<\/span><\/a><\/li>',r+='<li class="pagination-next"><a href="javascript:void(0);" onclick="previousnextclick(2);return false;" class="circleicn"><em class="fa fa-forward"><\/em><span class="show-for-sr">If pagination next forward click<\/span><\/a><\/li>'):(r+='<li class="pagination-next" style="display:none;"><a href="javascript:void(0);" onclick="previousnextclick(1);return false;" class="circleicn caret"><em class="fa fa-caret-right"><\/em><span class="show-for-sr">Else pagination next click<\/span><\/a><\/li>',r+='<li class="pagination-next" style="display:none;"><a href="javascript:void(0);" onclick="previousnextclick(2);return false;" class="circleicn"><em class="fa fa-forward"><\/em><span class="show-for-sr">Else pagination next forward click<\/span><\/a><\/li>')}$(i).html(r)}function setpage(n){var t,i,r,u,f;if($(".current").removeClass("current"),$(".pno"+n).addClass("current"),$(".pagination - next").removeClass("disabled"),n===1?$(".pagination-previous").hide():$(".pagination-previous").show(),n!==totalPages?$(".pagination-next").show():$(".pagination-next").hide(),totalPages>6)if(n+1===totalPages||n===totalPages){for($(".all").hide(),t=1;t<=4;t++)$(".pno"+t).show();for($($(".pno4")[0]).after($(".ellipsis")[0]),$(".ellipsis").length>1&&$(".pno4").length>1&&$($(".pno4")[1]).after($(".ellipsis")[1]),i=totalPages-1;i<=totalPages;i++)$(".pno"+i).show()}else if($(".all").hide(),n<totalPages-1){if($(".all").hide(),n+3===totalPages||n+2===totalPages?($($(".pno"+(n-3))[0]).after($(".ellipsis")[0]),$(".ellipsis").length>1&&$(".pno"+(n-3)).length>1&&$($(".pno"+(n-3))[1]).after($(".ellipsis")[1])):($($(".pno"+(totalPages-2))[0]).after($(".ellipsis")[0]),$(".ellipsis").length>1&&$(".pno"+(totalPages-2)).length>1&&$($(".pno"+(totalPages-2))[1]).after($(".ellipsis")[1])),n+6<=totalPages)for(r=n;r<n+3;r++)$(".pno"+r).show();else for(u=n-2;u<=n+1;u++)$(".pno"+u).show();for(f=totalPages-1;f<=totalPages;f++)$(".pno"+f).show()}}function previousnextclick(n){if((n!==-1&&n!==-2||current!==1)&&(n!==1&&n!==2||current!==totalPages)){var t=0;t=current+n;n===-2&&(t=1);n===2&&(t=totalPages);pageclick(t)}}function isValidEmail(n){if(n==="")return!1;return/^\S+@\S+\.\S+$/.test(n)}function validateForm(){var n,t,i,r,u,f;return $(".span-val").remove(),$(".span-val").remove(),n=!0,$(".req-validation").each(function(t,i){var r=$(i).val();if(r===null||r===""){var u=$(i).data("msg"),f="The "+u+" field is required.",e="<span class='field-validation-error text-danger span-val'>"+f+"<\/span>";$(i).parent().append(e);n===!0&&$(i).focus();n=!1}}),$(".reg-validation").each(function(t,i){var r=$(i).val(),u=$(i).data("msg"),f=!0,e,o;u=="Email"&&r!=null&&r!=""&&(f=isValidEmail(r));u==="Phone"&&r===""&&(f=!1);f||(e="The "+u+" is invalid.",o="<span class='field-validation-error text-danger span-val'>"+e+"<\/span>",$(i).parent().append(o),n==!0&&$(i).focus(),n=!1)}),$(".compare-validation").each(function(t,i){var r=$(i).val(),u=$(i).data("comfield"),f=$("#"+u+"").val(),s=$(i).data("msg"),h=$("#"+u+"").data("msg"),e,o;r!=""&&f!=""&&r!=f&&(e="The "+h+" and "+s+" do not match.",o="<span class='field-validation-error text-danger span-val'>"+e+"<\/span>",$(i).parent().append(o),n==!0&&$(i).focus(),n=!1)}),$(".password-validation").each(function(t,i){var r=$(i).val(),u="",h="",f=r.match(/[a-zA-Z]+/g),e=r.match(/\d+/g),c=$(i).data("msg"),o,s;f!=null&&(u=f.join(""));e!=null&&(h=e.join(""));(u.length<3||r.length>50)&&(o="Password you entered is in a incorrect format: "+c+" At least 3 letters",s="<div class='field-validation-error text-danger span-val'>"+o+"<\/div>",$(i).parent().append(s),n==!0&&$(i).focus(),n=!1)}),$(".chkreq-validation").each(function(t,i){var o=$(i).val(),e=$(i).data("msg"),r=$(i).data("controlname"),u,f;r&&$('[name="'+r+'"]:checked').length<1&&(u=e,f="<span class='field-validation-error text-danger span-val'>"+u+"<\/span>",$(i).append(f),n==!0&&$(i).focus(),n=!1)}),t=$("#AvatarImage").get(0).files,t.length>0&&(i=t[0],r=/image-*/,i.type.match(r)||(u="Invalid image format.",f="<span class='field-validation-error text-danger span-val'><br/>"+u+"<\/span>",$("#AvatarImage").parent().append(f),n=!1)),n}function setprofile(n){if($("#FirstName").val(n.FirstName),$("#LastName").val(n.LastName),$("#MiddleName").val(n.MiddleName),$("#PrefixSelected").val(n.PrefixSelected),$("#Organization").val(n.Organization),$("#WorkPhone").val(n.WorkPhone),$("#Title").val(n.Title),$("#Street1").val(n.Street1),$("#Street2").val(n.Street2),$("#Street3").val(n.Street3),$("#City").val(n.City),$("#StateProvinceSelected").val(n.StateProvinceSelected),$("#Zip").val(n.Zip),$("#CountrySelected").val(n.CountrySelected),$("#nlid").val(n.NemaLinkUserId),n.Image64string!=null&&n.Image64string!=""){var t=n.Image64string;$("#imgstring").val(t);$("#ImagePreview")[0].src="data:image;base64,"+t;$("#ImagePreview").show()}else $("#ImagePreview").hide()}var current=1,totalPages=0;$(document).ready(function(){var n=$("#NoofPages").val();n!==undefined&&n!==null&&n!==""&&(totalPages=parseInt(n))});$("#Email").change(function(){var r=$("#Email").parent().children().last().hasClass("field-validation-error"),n,t,i;r&&$("#Email").parent().children().last().remove();$(".fail").hide();$(".fail").html("");$("#Email").val()!=""&&(valid=isValidEmail($("#Email").val()),valid?(i=$("#Email").parent().children().last().hasClass("field-validation-error"),i&&$("#Email").parent().children().last().remove(),$.ajax({type:"GET",url:"/Registration/GetUserModel?email="+encodeURIComponent($("#Email").val()),contentType:"application/json; charset=utf-8",beforeSend:function(){$(".ajax-loader-news").show()},success:function(n){var r,i,t;n.um!=null?(setprofile(n.um),$("html, body").animate({scrollTop:450},"slow")):n.Message!=""&&(r="<span class='field-validation-error text-danger span-val'>"+n.Message+"<\/span>",$("#Email").parent().append(r));i=$("#hdnIntelligenceSubscriptionId").val();i!=""&&(t=$(":checkbox[value="+i+"]"),t.length>0&&(n.IsNemaMember?t.removeAttr("disabled"):t.attr("disabled",!0)));$(".ajax-loader-news").hide()},complete:function(){$(".ajax-loader-news").hide()},error:function(n){$(".ajax-loader-news").hide();$(".fail").show();$(".fail").html(n.responseJSON.ResponseStatus.Message);toastr.error(n.responseJSON.ResponseStatus.Message,"Error")}})):(n="The Email is invalid.",t="<span class='field-validation-error text-danger span-val'>"+n+"<\/span>",$("#Email").parent().append(t)))})