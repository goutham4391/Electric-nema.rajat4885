function GetCodeAlertsperYear(n,t){$.ajax({type:"GET",url:"/CodeAlerts/GetCodeAlerts?Year="+n,contentType:"application/json; charset=utf-8",beforeSend:function(){$(".ajax-loader-news").show()},success:function(n){console.log(n);$("#example-tabs li").removeClass("is-active");$(t).parent("li").addClass("is-active");$("#CodeAlertsData")&&($("#CodeAlertsData").empty(),$("#CodeAlertsData").append(GenerateData(n)));$(".ajax-loader-news").hide()},complete:function(){$(".ajax-loader-news").hide()},error:function(){$(".ajax-loader-news").hide()}})}function GenerateData(n){var t="";return t+='<div class="tabs-panel is-active" id="panel1" role="tabpanel" style="padding-left:0;">',t+='<div class="tab-container">',t+='<h3 class="mb-1">Recent Code Activity<\/h3>',$.each(n.CodeAlerts,function(n,i){t+='<div><span class="labelbold">'+i.Title+"<\/span><\/div>";t+=i.Description.Length<248?"<div>"+i.Description+"<\/div>":"<div>"+i.Description+'<span><a href="'+i.Url+'" aria-label="'+i.Title+'">More >><\/a><\/span><\/div>'}),t+="<\/div>",t+="<\/div>"}$(document).ready(function(){$("#example-tabs li:first-child").addClass("is-active")})